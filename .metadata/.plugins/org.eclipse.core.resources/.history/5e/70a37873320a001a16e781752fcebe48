#ifndef TUBIIADDRESSES_H_
#define TUBIIADDRESSES_H_

#define BURSTTRIG_BASEADDR 0x43C12000
#define BURSTTRIG_HIGHADDR 0x43C12FFF

#define TUBIIPGT_BASEADDR 0x43C16000
#define TUBIIPGT_HIGHADDR 0x43C16FFF

#define CLOCKLOGIC_BASEADDR 0x43C03000
#define CLOCKLOGIC_HIGHADDR 0x43C03FFF

#define COMBOTRIG_BASEADDR 0x43C04000
#define COMBOTRIG_HIGHADDR 0x43C04FFF

#define COUNTDISP_BASEADDR 0x43C05000
#define COUNTDISP_HIGHADDR 0x43C05FFF

#define ELLIECONTROL_BASEADDR 0x43C17000
#define ELLIECONTROL_HIGHADDR 0x43C17FFF

#define FIFOREADOUT_BASEADDR 0x43C02000
#define FIFOREADOUT_HIGHADDR 0x43C02FFF

#define GENERICDELAY_BASEADDR 0x43C0B000
#define GENERICDELAY_HIGHADDR 0x43C0BFFF

#define GENERICPULSER_BASEADDR 0x43C07000
#define GENERICPULSER_HIGHADDR 0x43C07FFF

#define GTDELAY_BASEADDR 0x43C0C000
#define GTDELAY_HIGHADDR 0x43C0CFFF

#define GTID_BASEADDR 0x43C15000
#define GTID_HIGHADDR 0x43C15FFF

#define MZHAPPY_BASEADDR 0x43C10000
#define MZHAPPY_HIGHADDR 0x43C10FFF

#define COUNTLENGTHEN_BASEADDR 0x43C0F000
#define COUNTLENGTHEN_HIGHADDR 0x43C0FFFF

#define DELAYLENGTHEN_BASEADDR 0x43C0A000
#define DELAYLENGTHEN_HIGHADDR 0x43C0AFFF

//#define SPEAKLENGTHEN_BASEADDR 0x43C14000
//#define SPEAKLENGTHEN_HIGHADDR 0x43C14FFF

#define PRESCALETRIG_BASEADDR 0x43C01000
#define PRESCALETRIG_HIGHADDR 0x43C01FFF

#define SHIFTREGS_BASEADDR 0x43C06000
#define SHIFTREGS_HIGHADDR 0x43C06FFF

#define SMELLIEDELAY_BASEADDR 0x43C0D000
#define SMELLIEDELAY_HIGHADDR 0x43C0DFFF

#define SMELLIEPULSER_BASEADDR 0x43C08000
#define SMELLIEPULSER_HIGHADDR 0x43C08FFF

#define SPEAKERSCALE_BASEADDR 0x43C00000
#define SPEAKERSCALE_HIGHADDR 0x43C00FFF

#define TELLIEDELAY_BASEADDR 0x43C0E000
#define TELLIEDELAY_HIGHADDR 0x43C0EFFF

#define TELLIEPULSER_BASEADDR 0x43C11000
#define TELLIEPULSER_HIGHADDR 0x43C11FFF

#define TRIGGEROUT_BASEADDR 0x43C09000
#define TRIGGEROUT_HIGHADDR 0x43C09FFF

#define TRIGGERSPLIT_BASEADDR 0x43C14000
#define TRIGGERSPLIT_HIGHADDR 0x43C14FFF

#define TRIGWORDDELAY_BASEADDR 0x43C13000
#define TRIGWORDDELAY_HIGHADDR 0x43C13FFF

#define ANTICOINCTRIG_BASEADDR 0x43C18000
#define ANTICOINCTRIG_HIGHADDR 0x43C18FFF

#define COINCTRIG_BASEADDR 0x43C19000
#define COINCTRIG_HIGHADDR 0x43C19FFF

#define ASYNCTOGGLE_BASEADDR 0x43C20000
#define ASYNCTOGGLE_HIGHADDR 0x43C20FFF

#define TUBIICLOCK_BASEADDR 0x43C21000
#define TUBIICLOCK_HIGHADDR 0x43C21FFF

#define MZCLOCK_BASEADDR 0x43C22000
#define MZCLOCK_HIGHADDR 0x43C22FFF

#define CLOCKCOMP_BASEADDR 0x43C23000
#define CLOCKCOMP_HIGHADDR 0x43C23FFF

#define DELAYCONTROL_BASEADDR 0x43C24000
#define DELAYCONTROL_HIGHADDR 0x43C24FFF

#endif /* TUBIIADDRESSES_H_ */
